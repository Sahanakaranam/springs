<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="com.xworkz.cosmetic.Cosmetic" id="cosmetic">
	<constructor-arg value="mac"></constructor-arg>
	<property name="price" value="1000"></property>
	</bean>
	
	<bean class="java.lang.Integer" id="integer">
	<constructor-arg value="10"></constructor-arg>
	</bean>
	
	<bean class="java.lang.String" id="string">
	<constructor-arg value="sahana is a developer"></constructor-arg>
	</bean>
	<bean class="java.lang.Float" id="float">
	<constructor-arg value="10.0f"></constructor-arg>
	</bean>
	<bean class="java.lang.Double" id="double">
	<constructor-arg value="1000.00d"></constructor-arg>
	</bean>
	<bean class="java.lang.Boolean" id="boolean">
	<constructor-arg value="True"></constructor-arg>
	</bean>
	<bean class="java.lang.Long" id="long">
	<constructor-arg value="10000"></constructor-arg>
	</bean>
	<bean class="java.lang.Short" id="short">
	<constructor-arg value="20"></constructor-arg>
	</bean>
	<bean class="java.lang.Byte" id="byte">
	<constructor-arg value="1"></constructor-arg>
	</bean>
	
	</beans>